<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>TestW</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<context-param>
		<param-name>jmxPlanPath</param-name>
		<param-value>/Library/WebServer/Documents/script/</param-value>
	</context-param>

	<context-param>
		<param-name>jmxReportPath</param-name>
		<param-value>/Library/WebServer/Documents/report/</param-value>
	</context-param>

	<context-param>
		<param-name>jmxPlanTemple</param-name>
		<param-value>/Users/perfermance/JmeterTest/script/jmxPlanTemple.jmx</param-value>
	</context-param>
	
	<context-param>
		<param-name>baseJmeterPath</param-name>
		<param-value>/Users/perfermance/JmeterTest/apache-jmeter-2.13/bin/</param-value>
	</context-param>
	
	<context-param>
		<param-name>baseJtlPath</param-name>
		<param-value>/Library/WebServer/Documents/jtl/</param-value>
	</context-param>
</web-app>